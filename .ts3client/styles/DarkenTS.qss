/* ---- TreeTab/Tree View ---- */

/*QWidget {
	border: none;	NO IDEA WHY THIS IS HERE, LEAVING IT AS REMINDER IN CASE SOMETHING BREAKS
}*/

QWidget::pane {
	border: 0px solid transparent;
}

QTreeView {
	qproperty-animated: 1;
	border: 1px solid rgb(29,29,29);
	background-color: rgb(29,29,29);
		alternate-background-color: rgb(29,29,29);	
	outline: none;
}

QTreeView::item:hover {
	background-color: rgba(62,79,94,0.5);
}
	
QTreeView::item:selected:active,
QTreeView::item:selected:!active {
	background-color: rgb(62,79,94);
	color: rgb(216,216,216);
}
	
QTreeView::branch:open:has-children {
	image: url('styles:DarkenTS/gfx/gui/arrows/branch_open.svg');
}

QTreeView::branch:open:has-children:hover {
	image: url('styles:DarkenTS/gfx/gui/arrows/branch_open_hover.svg');
}

QTreeView::branch:closed:has-children {			
	image: url('styles:DarkenTS/gfx/gui/arrows/branch_closed.svg');
}

QTreeView::branch:closed:has-children:hover {
	image: url('styles:DarkenTS/gfx/gui/arrows/branch_closed_hover.svg');
}

QTreeView:disabled,
QHeaderView:disabled,
QTreeView::item:selected:disabled {
	background-color: rgb(40,40,40);
	color: rgb(72,72,72);
}

QHeaderView::section {
	padding-left: 4px;
}

QHeaderView::section:middle,
QHeaderView::section:last {
	border-left: 1px solid rgb(29,29,29);
}

