	background-color: rgb(62,79,94);
}

QTabBar::tab:hover {	 
	background-color: rgb(48,56,64);
}

QTabBar::tab:disabled {
	background-color: rgba(40,40,40);
	color: rgb(72,72,72);
}

QTabBar::tab:middle {
	border: 1px solid transparent;
	border-left-color: rgb(29,29,29);
}

QTabBar::tab:last {
	border: 1px solid transparent;
	border-left-color: rgb(29,29,29);
}

PermissionsWidget QTabBar::tab:middle,
