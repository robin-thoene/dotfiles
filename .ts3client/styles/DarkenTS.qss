	image: url('styles:DarkenTS/gfx/gui/controls/checkbox_checked_disabled.svg');
}

QRadioButton::indicator:checked {
	image: url('styles:DarkenTS/gfx/gui/controls/radiobutton_checked.svg');
}

QRadioButton::indicator:checked:disabled {
	image: url('styles:DarkenTS/gfx/gui/controls/radiobutton_checked_disabled.svg');
}

QListView::indicator:indeterminate,
QCheckBox::indicator:indeterminate,
QGroupBox::indicator:indeterminate,
QTreeView::indicator:indeterminate {
	image: url('styles:DarkenTS/gfx/gui/controls/checkbox_checked_indeterminate.svg');
}

